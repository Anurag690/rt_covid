    <style>
        .myTooltip {
  position: relative;
  display: inline-block;
  border-bottom: 1px dotted black;
}

.myTooltip .myTooltipText {
  visibility: hidden;
  width: 120px;
  background-color: black;
  color: #fff;
  text-align: center;
  border-radius: 6px;
  padding: 5px 0;
  position: absolute;
  z-index: 1;
  bottom: 150%;
  left: 50%;
  margin-left: -60px;
}

.myTooltip .myTooltipText::after {
  content: "";
  position: absolute;
  top: 100%;
  left: 50%;
  margin-left: -5px;
  border-width: 5px;
  border-style: solid;
  border-color: black transparent transparent transparent;
}

.myTooltip:hover .myTooltipText {
  visibility: visible;
}
a{
    color: black;
}
    </style>
